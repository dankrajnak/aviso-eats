{
	"name": "aviso-eats",
	"version": "1.0.2",
	"license": "MIT",
	"bin": "dist/bundle.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"clean": "rimraf dist",
		"build": "yarn clean && rollup -c rollup.config.ts",
